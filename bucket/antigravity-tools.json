{
  "version": "3.3.9",
  "description": "A modern, cross-platform account manager for Antigravity. Features one-click account switching, automatic backups, and a sleek GUI built with Flet",
  "homepage": "https://github.com/lbjlaq/Antigravity-Manager",
  "license": "MIT",
  "architecture": {
    "64bit": {
      "url": "https://github.com/lbjlaq/Antigravity-Manager/releases/download/v3.3.9/Antigravity.Tools_3.3.9_x64-setup.exe#/dl.7z"
    }
  },
  "installer": {
    "script": [
      "Expand-7zipArchive -Path \"$dir\\$*\" -DestinationPath \"$dir\" -Removal",
      "Get-ChildItem \"$dir\" -Filter '*.nsi' | Remove-Item -Force",
      "Get-ChildItem \"$dir\" -Include '*.exe' -Recurse | Where-Object { $_.Name -notmatch 'uninstall|setup' } | Move-Item -Destination \"$dir\\Antigravity.Tools.exe\" -Force"
    ]
  },
  "bin": "Antigravity.Tools.exe",
  "shortcuts": [
    [
      "Antigravity.Tools.exe",
      "Antigravity Tools"
    ]
  ],
  "checkver": {
    "url": "https://api.github.com/repos/lbjlaq/Antigravity-Manager/releases/latest",
    "jsonpath": "$.tag_name",
    "regex": "v([\\d.]+)"
  },
  "autoupdate": {
    "architecture": {
      "64bit": {
        "url": "https://github.com/lbjlaq/Antigravity-Manager/releases/download/v$version/Antigravity.Tools_$version_x64-setup.exe#/dl.7z"
      }
    }
  }
}