{
  "version": "0.1.37",
  "description": "Kunkun - application launcher / launcher-like tool",
  "homepage": "https://github.com/kunkunsh/kunkun",
  "license": "MIT",
  "architecture": {
    "64bit": {
      "url": "https://github.com/kunkunsh/kunkun/releases/download/Kunkun-v0.1.37/kunkun_0.1.37_x64-setup.exe",
      "hash": "sha256:e2c856ea8124e375a6cb2ac80df07cdc81aa5741c2721ee0fead56c3a8a5e96d"
    }
  },
  "noextract": true,
  "depends": "7zip",
  "installer": {
    "script": [
      "& 7z x \"$dir\\$fname\" -o\"$dir\" -y",
      "# 如果发现嵌套的 installer.exe，再解一次并移除中间文件",
      "Get-ChildItem \"$dir\" -Filter \"*setup*.exe\" | ForEach-Object { & 7z x $_.FullName -o\"$dir\" -y; Remove-Item $_.FullName -Force }",
      "Remove-Item -Path \"$dir\\$fname\" -Force"
    ]
  },
  "bin": "kunkun.exe",
  "shortcuts": [
    [
      "kunkun.exe",
      "Kunkun"
    ]
  ],
  "checkver": {
    "github": "kunkunsh/kunkun"
  },
  "autoupdate": {
    "architecture": {
      "64bit": {
        "url": "https://github.com/kunkunsh/kunkun/releases/download/Kunkun-v$version/kunkun_$version_x64-setup.exe"
      }
    }
  }
}
