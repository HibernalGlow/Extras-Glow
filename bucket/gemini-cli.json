{
    "version": "0.8.0",
    "description": "Gemini CLI - A command-line interface for Google's Gemini AI models",
    "homepage": "https://github.com/google-gemini/gemini-cli#readme",
    "license": "Apache-2.0",
    "architecture": {
        "64bit": {
            "url": "https://mirrors.cloud.tencent.com/npm/@google/gemini-cli/-/gemini-cli-0.8.0.tgz",
            "hash": "b270ce72e33f45dbc2ef99dea50b207cf51137ef02b4e5c578bd943546e6d694"
        }
    },
    "extract_dir": "package",
    "installer": {
        "script": [
            "cd \"$dir\"",
            "yarn install --production",
            "Set-Content -Path \"$dir\\gemini-cli.cmd\" -Value \"@node.exe \"\"$dir\\dist\\index.js\"\" %*\"",
            "Set-Content -Path \"$dir\\gemini.cmd\" -Value \"@node.exe \"\"$dir\\dist\\index.js\"\" %*\""
        ]
    },
    "bin": [
        "gemini-cli.cmd",
        "gemini.cmd"
    ],
    "checkver": {
        "url": "https://registry.npmjs.org/@google/gemini-cli",
        "jsonpath": "$.dist-tags.latest"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://mirrors.cloud.tencent.com/npm/@google/gemini-cli/-/gemini-cli-$version.tgz"
            }
        }
    }
}
