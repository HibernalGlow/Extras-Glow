    "homepage": "https://github.com/google-gemini/gemini-cli#readme",
    "license": "Apache-2.0",
    "architecture": {
        "64bit": {
            "url": "https://mirrors.cloud.tencent.com/npm/@google/gemini-cli/-/gemini-cli-0.21.0.tgz",
