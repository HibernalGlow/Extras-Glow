{
    "version": "0.20.2",
    "description": "Gemini CLI - A command-line interface for Google's Gemini AI models",
    "homepage": "https://github.com/google-gemini/gemini-cli#readme",
    "license": "Apache-2.0",
    "architecture": {
        "64bit": {
            "url": "https://mirrors.cloud.tencent.com/npm/@google/gemini-cli/-/gemini-cli-0.20.2.tgz",
            "hash": "4d9da964f0380907d8c839ea8bd80589e5015d308ae802b7a05818f017d29ccb"
        }
    },
    "extract_dir": "package",
    "installer": {
        "script": [
            "cd \"$dir\"",
            "pnpm install --production --no-optional --ignore-scripts",
            "Remove-Item \"$dir\\node_modules\\.bin\" -Recurse -Force -ErrorAction SilentlyContinue"
        ]
    },
    "bin": "gemini-cli.cmd",
    "post_install": [
        "if (!(Test-Path \"$dir\\gemini-cli.cmd\")) {",
        "    $cmd = \"@echo off",
        "node.exe \\\"%~dp0dist\\index.js\\\" %*\"",
        "    Set-Content -Path \"$dir\\gemini-cli.cmd\" -Value $cmd -Encoding ASCII",
        "}"
    ],
    "checkver": {
        "url": "https://registry.npmjs.org/@google/gemini-cli",
        "jsonpath": "$.dist-tags.latest"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://mirrors.cloud.tencent.com/npm/@google/gemini-cli/-/gemini-cli-$version.tgz"
            }
        }
    }
}
